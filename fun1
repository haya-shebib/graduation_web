     <div class="post">
            <?php echo $row['video_title']; ?> <br>

            <?php
            $res=query("SELECT * FROM likes  WHERE user_id = 1 AND post_id = " . $row['id']."");
            if(mysqli_num_rows($res)==1){?>
            <span><a href="" class="unlike" id="<?php echo  $row['id']; ?> "> not like</a></span>

            <?php } else {  ?>
