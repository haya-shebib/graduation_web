<?php require_once("../resources/confiq.php");


if(isset($_POST['liked'])){
    redirect("admin");

    $post_id=$_POST['post_id'];
    $result=query("SELECT * FROM videos WHERE  post_id=$post_id ");
    confirm($result);
    $row=fetch_array($result);
    $n=$row['like'];
    $upd=query("UPDATE videos SET like =$n+1 WHERE id=$post_id");
    confirm($upd);
    $ins=query("INSERT INTO like (user_id,post_id)VALUES (1, $post_id) ");
    confirm($ins);
    exit;
}?>

